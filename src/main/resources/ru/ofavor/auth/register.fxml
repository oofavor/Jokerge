<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ru.ofavor.auth.RegisterController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
         <children>
            <PasswordField fx:id="passwordField" layoutX="227.0" layoutY="188.0" promptText="password" />
            <TextField fx:id="emailField" layoutX="227.0" layoutY="140.0" promptText="email" />
            <Text layoutX="227.0" layoutY="72.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Register">
               <font>
                  <Font size="42.0" />
               </font>
            </Text>
            <CheckBox fx:id="isStore" layoutX="226.0" layoutY="284.0" mnemonicParsing="false" text="Store" />
            <CheckBox fx:id="isFactory" layoutX="315.0" layoutY="284.0" mnemonicParsing="false" text="Factory" />
            <Button layoutX="226.0" layoutY="314.0" mnemonicParsing="false" onAction="#register" prefHeight="25.0" prefWidth="149.0" text="Register" />
            <Button layoutX="226.0" layoutY="351.0" mnemonicParsing="false" onAction="#switchToLogin" prefHeight="25.0" prefWidth="149.0" text="Switch to login" />
            <TextField fx:id="faxField" layoutX="227.0" layoutY="235.0" prefHeight="25.0" prefWidth="151.0" promptText="fax (keep empty if factory)" />
         </children></AnchorPane>
  </children>
</VBox>
