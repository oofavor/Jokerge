<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ru.ofavor.auth.LoginController">
  <children>
      <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
         <children>
            <PasswordField fx:id="passwordField" layoutX="227.0" layoutY="188.0" promptText="password" />
            <TextField fx:id="emailField" layoutX="227.0" layoutY="140.0" promptText="email" />
            <Text layoutX="249.0" layoutY="72.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Login">
               <font>
                  <Font size="42.0" />
               </font>
            </Text>
            <Button layoutX="226.0" layoutY="234.0" mnemonicParsing="false" onAction="#login" prefHeight="25.0" prefWidth="149.0" text="Login" />
            <Button layoutX="228.0" layoutY="274.0" mnemonicParsing="false" onAction="#switchToRegister" prefHeight="25.0" prefWidth="149.0" text="Switch to register" />
            <Button layoutX="228.0" layoutY="351.0" mnemonicParsing="false" onAction="#loginGuest" prefHeight="25.0" prefWidth="149.0" text="Guest" />
         </children>
      </AnchorPane>
  </children>
</VBox>
